<section class="logolist--container custompage-sectionpad" style="background-color: {{ section.settings.background_color }};">
  {% if section.settings.title != blank or section.settings.subheading != blank %}
    <div class="home-section--title-container" >
    {% if section.settings.title != blank %}
      <h2 class="home-section--title" style="color: {{ section.settings.heading_color }};">
        {{ section.settings.title | escape }}
      </h2>
    {% endif %}
    {% if section.settings.subheading != blank %}
      <div class="home-section--subheading" style="color: {{ section.settings.subheading_color }};">
        {{ section.settings.subheading }}
      </div>
    {% endif %}
    </div>
  {% endif %}

    <div class="home-section--content logolist--inner">
        {% if block.settings.logoone_show %}
            <div class="logolist--item" {{ block.shopify_attributes }}>
                {% if block.settings.logoone_link != blank %}<a class="logolist--link" href="{{block.settings.logoone_link | escape}}" target="_blank">{% endif %}
                {% if block.settings.logoone_image %}
                    {% include 'rimg', img: block.settings.logoone_image, class: 'logolist--image', size: '160x160', lazy: settings.lazy_load, canvas: settings.lazy_load %}
                {% else %}
                    {{ 'image' | placeholder_svg_tag: 'placeholder--image logolist--image' }}
                {% endif %}
                {% if block.settings.logoone_link != blank %}</a>{% endif %}
            </div>
        {% endif %}
        {% if block.settings.logotwo_show %}
            <div class="logolist--item" {{ block.shopify_attributes }}>
                {% if block.settings.logotwo_link != blank %}<a class="logolist--link" href="{{block.settings.logotwo_link | escape}}" target="_blank">{% endif %}
                {% if block.settings.logotwo_image %}
