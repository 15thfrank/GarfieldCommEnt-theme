            </div>
        {% endif %}
        {% if block.settings.logofive_show %}
            <div class="logolist--item" {{ block.shopify_attributes }}>
                {% if block.settings.logofive_link != blank %}<a class="logolist--link" href="{{block.settings.logofive_link | escape}}" target="_blank">{% endif %}
                {% if block.settings.logofive_image %}
