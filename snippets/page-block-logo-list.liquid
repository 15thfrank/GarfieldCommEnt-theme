                {% endif %}
                {% if block.settings.logothree_link != blank %}</a>{% endif %}
            </div>
        {% endif %}
        {% if block.settings.logofour_show %}
            <div class="logolist--item" {{ block.shopify_attributes }}>
                {% if block.settings.logofour_link != blank %}<a class="logolist--link" href="{{block.settings.logofour_link | escape}}" target="_blank">{% endif %}
