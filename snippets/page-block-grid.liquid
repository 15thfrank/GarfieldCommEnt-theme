      {% if block.settings.gridone_text_alignment contains 'top' %}
        {% assign text_position = 'top' %}
      {% elsif block.settings.gridone_text_alignment contains 'bottom' %}
        {% assign text_position = 'bottom' %}
      {% else %}
        {% assign text_position = block.settings.gridone_text_alignment | remove: 'center-' %}
      {% endif %}

    .sectionuse-{{block.settings.sectionuse}} .promo-block-grid--index-1 .promo-block-grid--content {
        {% if block.settings.gridone_overlay_style == 'solid' %}
