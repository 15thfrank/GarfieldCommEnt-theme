<section class="sectionuse-{{block.settings.sectionuse}} promo-grid--container custompage-section" style="background-color: {{ block.settings.background_color }};" {{ block.shopify_attributes }}>
  {% if block.settings.title != blank or block.settings.subheading != blank %}
    <div class="home-section--title-container" >
    {% if block.settings.title != blank %}
      <h2 class="home-section--title" style="color: {{ block.settings.heading_color }};">
        {{ block.settings.title | escape }}
      </h2>
    {% endif %}
    {% if block.settings.subheading != blank %}
      <div class="home-section--subheading" style="color: {{ block.settings.subheading_color }};">
        {{ block.settings.subheading }}
      </div>
    {% endif %}
    </div>
  {% endif %}

  {%- capture classes -%}
    home-section--content
  {%- endcapture -%}

  <div class="{{ classes | strip_newlines }}">
    <!--Desktop blocks begin here-->
    <div class="promo-grid--inner">
      <!--Grid one begins-->
      {% if block.settings.gridone_show %}
        <article class="promo-block-grid promo-block-grid--index-1 promo-grid--{{block.settings.gridone_promo_width}} desktop-block"{% if block.settings.gridone_image %}data-rimg="loaded" data-rimg-scale="1" data-rimg-max="4096x2731" data-rimg-crop=""{% endif %}>
