            <div class="promo-block-grid--content  promo-block-grid--content-align-{{block.settings.gridone_text_alignment}}  promo-grid--maintain-aspect-ratio  promo-block-grid--expanded  promo-block-grid--unlinked  onboarding">
              <div class="promo-block-grid--content-wrapper">
                <div class="promo-block-grid--preheading">{{ block.settings.gridone_preheading }}</div>
                <h2 class="promo-block-grid--header">{{block.settings.gridone_title}}</h2>
                <p class="promo-block-grid--text">{{block.settings.gridone_text}}</p>
                {% if block.settings.gridone_button_text.size>0 and block.settings.gridone_button_style != "signpost"%}
                  <span class="{% if block.settings.gridone_button_style == "primary" or block.settings.gridone_button_style == "primary_icon" %}button-primary {%endif%} {% if block.settings.gridone_button_style == "secondary" or block.settings.gridone_button_style == "secondary_icon" %}button-secondary {%endif%} promo-block-grid--button {% if block.settings.gridone_button_style == "primary_icon" or block.settings.gridone_button_style == "secondary_icon" %}has-icon{%endif%}">
                    <span class="button-text">{{block.settings.gridone_button_text}}</span>
                    {% if block.settings.gridone_button_style == "primary_icon" or block.settings.gridone_button_style == "secondary_icon" %}
                      <span class="button-icon">
                        <svg class="icon-chevron-signpost " aria-hidden="true" focusable="false" role="presentation" xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none">          <title>Chevron down icon</title>        <path d="M4 5.75L7 8.75L10 5.75" stroke="currentColor" stroke-width="1.25"></path>      <circle cx="7" cy="7" r="6.375" stroke="currentColor" stroke-width="1.25"></circle>    </svg>
                      </span>
                    {% endif %}
                  </span>
                {% endif %}
                {% if block.settings.gridone_button_style == "signpost"%}
                  <span class="button-signpost promo-block-grid--button">
                    <span class="button-text">{{block.settings.gridone_button_text}}</span>
